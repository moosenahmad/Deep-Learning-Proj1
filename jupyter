import matplotlib.pyplot as plt
import numpy as np

csv = np.loadtxt(open("Salary_Data.csv", "rb"), delimiter=",", skiprows=1)

print("Fifth row: {}".format(csv[4]))
csvsize = len(csv)
i = 1
x = []
y = []
while(i < csvsize):
    x.append(csv[i][0])
    y.append(csv[i][1])
    i+=1

plt.plot(x,y)
plt.xlabel('YearsExperience')
plt.ylabel('Salary')
plt.show()
